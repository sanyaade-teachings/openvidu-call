{
	"name": "openvidu-call-server",
	"version": "3.0.0-beta3",
	"description": "OpenVidu Call Server",
	"author": "OpenVidu",
	"license": "Apache-2.0",
	"homepage": "https://github.com/OpenVidu/openvidu-call#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/OpenVidu/openvidu-call.git"
	},
	"bugs": {
		"url": "https://github.com/OpenVidu/openvidu-call/issues"
	},
	"keywords": [
		"openvidu",
		"webrtc",
		"openvidu-call"
	],
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"start": "node dist/src/server.js",
		"dev:start": "cross-env USE_HTTPS=false SERVER_CORS_ORIGIN=* nodemon -I --exec node --experimental-specifier-resolution=node --loader ts-node/esm ./src/server.ts",
		"lint": "eslint src --fix",
		"format": "prettier --ignore-path .gitignore --write '**/*.{ts,js,json,md}'",
		"package": "npm run build && npm pack",
		"lib:sync-call-pro": "npm run package && cp openvidu-call-server-*.tgz ../../openvidu-call-pro/backend"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.673.0",
		"chalk": "5.3.0",
		"cors": "2.8.5",
		"dotenv": "16.4.5",
		"express": "4.21.0",
		"express-basic-auth": "1.2.1",
		"express-rate-limit": "^7.4.1",
		"inversify": "^6.2.1",
		"ioredis": "^5.4.2",
		"livekit-server-sdk": "2.6.2",
		"reflect-metadata": "^0.2.2",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"winston": "3.14.2"
	},
	"devDependencies": {
		"@types/chai": "4.3.19",
		"@types/cookie-parser": "1.4.7",
		"@types/cors": "2.8.17",
		"@types/express": "4.17.21",
		"@types/node": "^20.12.14",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.6",
		"@types/validator": "^13.12.2",
		"@typescript-eslint/eslint-plugin": "6.7.5",
		"@typescript-eslint/parser": "6.7.5",
		"chai": "4.3.10",
		"cross-env": "7.0.3",
		"eslint": "8.50.0",
		"eslint-config-prettier": "9.0.0",
		"nodemon": "3.1.7",
		"prettier": "3.3.3",
		"ts-node": "10.9.2",
		"typescript": "5.4.5"
	}
}
